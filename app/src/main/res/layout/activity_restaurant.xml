<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#BDBDBD"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.notnull.nihal.viewbhopal.Restaurants">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView2">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:showDividers="middle"
            android:divider="?android:dividerHorizontal"
            android:dividerPadding="10dp"
            android:orientation="vertical"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true">

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:text="BAKE N SHAKE"
                android:background="#BDBDBD"
                android:id="@+id/textView1"
                android:layout_alignParentTop="true"
                android:textSize="25sp"
                android:onClick="buttonFunction1"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="MOMO CAFE"
                android:onClick="buttonFunction6"
                android:background="#BDBDBD"
                android:id="@+id/textView2"
                android:textSize="25sp"
                android:clickable="true" />


            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="LAZEEZ HAKEEM"
                android:background="#BDBDBD"
                android:id="@+id/textView3"
                android:layout_below="@+id/textView2"
                android:textSize="25sp"
                android:onClick="buttonFunction3"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:text="BAPU KI KUTIA"
                android:background="#BDBDBD"
                android:id="@+id/textView4"
                android:layout_alignParentStart="true"
                android:textSize="25sp"
                android:onClick="buttonFunction2"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="DA PIZZERIA"
                android:background="#BDBDBD"
                android:id="@+id/textView5"
                android:textSize="25sp"
                android:onClick="buttonFunction4"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="#BDBDBD"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="SOORMA BHOPALI"
                android:onClick="buttonFunction5"
                android:id="@+id/textView6"
                android:textSize="25sp"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:background="#BDBDBD"
                android:layout_height="55dp"
                android:text="TEA TALKS"
                android:onClick="buttonFunction7"
                android:id="@+id/textView7"
                android:textSize="25sp"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:background="#BDBDBD"
                android:layout_height="55dp"
                android:text="SAGAR GAIRE"
                android:onClick="buttonFunction8"
                android:id="@+id/textView8"
                android:textSize="25sp"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="#BDBDBD"
                android:text="Kafe Kulture"
                android:onClick="buttonFunction9"
                android:id="@+id/textView9"
                android:textSize="25sp"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="#BDBDBD"
                android:text="WONDER BREAD"
                android:onClick="buttonFunction10"
                android:id="@+id/textView10"
                android:textSize="25sp"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="#BDBDBD"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="RICE BOWL"
                android:id="@+id/textView11"
                android:onClick="buttonFunction11"
                android:layout_alignParentStart="true"
                android:textSize="25sp"
                android:clickable="true"
                android:focusableInTouchMode="true"
                android:focusable="true"
                android:enabled="true" />

            <Button
                android:layout_width="match_parent"
                android:background="#BDBDBD"
                android:layout_height="55dp"
                android:text="HONG KONG"
                android:onClick="buttonFunction12"
                android:id="@+id/textView12"
                android:textSize="25sp"
                android:clickable="true" />

            <Button
                android:background="#BDBDBD"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:text="Amer Bakery Hut"
                android:onClick="buttonFunction13"
                android:id="@+id/textView13"
                android:textSize="25sp"
                android:clickable="true" />
            <Button
                android:layout_width="match_parent"
                android:background="#BDBDBD"
                android:layout_height="55dp"
                android:text="Natural"
                android:onClick="buttonFunction15"
                android:id="@+id/textView14"
                android:textSize="25sp"
                android:clickable="true" />

            <Button
                android:layout_width="match_parent"
                android:background="#BDBDBD"
                android:layout_height="55dp"
                android:text="Bhopal Express"
                android:onClick="buttonFunction14"
                android:id="@+id/textView15"
                android:textSize="25sp"
                android:clickable="true" />
        </LinearLayout>
    </ScrollView>

</RelativeLayout>
